{
  "name": "medical",
  "productName": "TMS",
  "version": "1.3.0",
  "description": "Application for electron tutorials",
  "repository": "https://github.com/dippy1988-deependra/electron-chatApp",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "build": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.example.app",
    "mac": {
      "category": "medical",
      "target": [
        "zip",
        "dmg"
      ],
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "build/icon.png"
    }
  },
  "author": "zeke.jimenea.wiselysoft@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "electron": "^5.0.6",
    "electron-builder": "^20.44.4",
    "electron-builder-squirrel-windows": "^21.0.3",
    "electron-is-dev": "^1.1.0"
  },
  "dependencies": {
    "dotenv": "^8.0.0",
    "electron-log": "^3.0.6",
    "electron-osx-sign": "^0.4.11",
    "electron-updater": "^4.0.6",
    "gh-got": "^8.1.0",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "node-env-file": "^0.1.8",
    "quickblox": "^2.12.2",
    "update-electron-app": "^1.4.2"
  }
}
