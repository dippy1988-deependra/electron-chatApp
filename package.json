{
  "name": "medical",
  "productName": "TMS",
  "version": "1.0.0",
  "description": "Application for electron tutorials",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --linux",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.example.app",
    "mac": {
      "category": "medical",
      "target": [
        "zip",
        "dmg"
      ],
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "snap"
      ],
      "icon": "build/icon.png"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ],
          "icon": "build/icon.ico"
        }
      ]
    }
  },
  "author": "zeke <zeke.jimenea.wiselysoft@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "electron": "^5.0.6",
    "electron-builder": "^20.44.4",
    "electron-builder-squirrel-windows": "^21.0.3",
    "electron-is-dev": "^1.1.0"
  },
  "dependencies": {
    "dotenv": "^8.0.0",
    "electron-log": "^3.0.6",
    "electron-updater": "^4.0.6",
    "gh-got": "^8.1.0",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "node-env-file": "^0.1.8",
    "quickblox": "^2.12.2"
  }
}
